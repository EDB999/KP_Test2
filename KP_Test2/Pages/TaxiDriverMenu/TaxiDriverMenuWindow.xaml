<Window x:Class="KP_Test2.Pages.TaxiDriverMenu.TaxiDriverMenuWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KP_Test2.Pages.TaxiDriverMenu"
        mc:Ignorable="d"
        Title="TaxiDriverMenu" Height="600" Width="1500">
    <Grid Height="600" VerticalAlignment="Top">
        <Button x:Name="MakeAnOrderButton" Height="300" Background="{x:Null}" Margin="80,150,1059,150">
            <Image Source="Images/DriverHistoryOrders.jpg"/>
        </Button>
        <Button x:Name="DriverHistoryOrdersButton" Height="300" Width="300" Click="DriverHistoryOrdersButton_Click" Background="{x:Null}" Margin="486,150,714,150">
            <Image Source="Images/DriverHistoryOrders.jpg"/>
        </Button>
        <Button x:Name="DriverCarButton" Height="300" Width="321" Click="DriverCarButton_Click" Background="{x:Null}" HorizontalAlignment="Left" Margin="1148,150,0,150">
            <Image Source="Images/DriverCar.jpg"/>
        </Button>
        <Button x:Name="DriverActivity" Height="300" Width="321" Click="DriverActivity_Click" Background="{x:Null}" HorizontalAlignment="Left" Margin="807,150,0,150">
            <Image Source="Images/DriverCar.jpg"/>
        </Button>
    </Grid>
</Window>
